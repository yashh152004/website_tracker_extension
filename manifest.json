{
  "manifest_version": 3,
  "name": "Time Tracker Pro",
  "version": "1.0.0",
  "description": "Track and visualize how much time you spend on websites daily.",
  "permissions": ["tabs", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
